server {
    listen 80;
    server_name infonigma.infopriceti.com.br;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/shcrubles/shcrubles;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/infonigma/socket;
    }
}
