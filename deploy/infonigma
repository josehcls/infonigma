server {
    listen 80;
    server_name 107.21.88.91;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/infonigma/infonigma;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/infonigma/socket;
    }
}
